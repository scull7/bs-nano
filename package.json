{
  "name": "bs-nano",
  "version": "0.1.0",
  "description": "BuckleScript bindings to the CouchDB nano interface",
  "scripts": {
    "build": "bsb -make-world",
    "clean": "bsb -clean-world",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "precommit": "lint-staged",
    "start": "bsb -make-world -w",
    "test": "yarn run clean; yarn run build; jest --coverage && yarn run coverage",
    "watch:jest": "jest --coverage --watchAll"
  },
  "lint-staged": {
    "*.{re,rei}": [
      "refmt --in-place",
      "git add"
    ]
  },
  "main": "src/Nano.bs.js",
  "repository": "git@github.com:scull7/bs-nano.git",
  "author": "Nathan Sculli <scull7@gmail.com>",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "bs-platform": "^2.2.3"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.2",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "nyc": "^11.6.0"
  },
  "dependencies": {
    "@glennsl/bs-json": "^1.3.1",
    "agentkeepalive": "^3.4.1",
    "bs-platform": "^3.0.0",
    "global": "^4.3.2",
    "nano": "^6.4.3"
  }
}
